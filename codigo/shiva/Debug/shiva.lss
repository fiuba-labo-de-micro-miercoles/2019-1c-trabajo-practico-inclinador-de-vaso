                                 ; DECLINACION_INIT:
                                 ; Funcion que setea el servo en la posicion de amyor inclinacion de la
                                 ; plataforma
                                 ;-------------------------------------------------------------------------
                                 .equ  POS_MAYOR_INCLINACION = 313 + 40			; valor para el Output Comapre Register para posicionar el servo en la mayor inclinacion
                                 
                                 declinacion_init:
000783 930f                      	push r16
                                 	
000784 e001                      	ldi  r16, HIGH(POS_MAYOR_INCLINACION)
000785 9300 0089                 	sts  OCR1AH, r16
                                 
000787 e601                      	ldi  r16, LOW(POS_MAYOR_INCLINACION)
000788 9300 0088                 	sts  OCR1AL, r16
                                 
00078a 910f                      	pop  r16
00078b 9508                      	ret
                                 
                                 ;-------------------------------------------------------------------------
                                 ; PROCESO_DECLINACION:
                                 ; funcion para el proceso de declinacion del vaso en funcion de la cantidad
                                 ; de liquido servido. La plataforma posee 32 posiciones de inclinacion y 
                                 ; el vaso comienza en la posicion 32, y comienza a declinarse cuando
                                 ; la cantidad de liquido servida es igual a la mitad de la medida elegida.
                                 ;-------------------------------------------------------------------------
                                 .def   TECHO_L = r20
                                 .def   TECHO_H = r21
                                 .def   PASO = r22
                                 
                                 proceso_declinacion:
00078c 930f                      	push  r16
00078d 931f                      	push  r17
00078e 934f                      	push  TECHO_L
00078f 935f                      	push  TECHO_H
000790 936f                      	push  PASO
                                 
000791 2d4b                      	mov   TECHO_L, MEDIDA_L
000792 2d5c                      	mov   TECHO_H, MEDIDA_H
000793 9556                      	lsr   TECHO_H									; dividimos la medida por 2
000794 9547                      	ror   TECHO_L									; en r16 ya va a quedar la mitad de la medida
                                 	
000795 2f64                      	mov   PASO, TECHO_L								; se divide el rango de valores que va desde MEDIDA/2 HASTA MEDIDA,
000796 9566                      	lsr   PASO										; por 32, que son las cantidad de posiciones que tiene la paltaforma.
000797 9566                      	lsr   PASO									
000798 9566                      	lsr   PASO
000799 9566                      	lsr   PASO
00079a 9566                      	lsr   PASO
                                 
                                 proceso_declinacion_lectura:
00079b dd64                      	rcall lectura_peso								; se lee un dato
00079c ddaf                      	rcall set_scale
00079d de20                      	rcall detectar_cancelacion
                                 
00079e 1635                      	cp    DATO_M, TECHO_H							; se compara el valor leido con el techo
00079f f3d8                      	brlo  proceso_declinacion_lectura				; mientras el dato sea menor que el techo se sigue leyendo
0007a0 1624                      	cp    DATO_L, TECHO_L
0007a1 f3c8                      	brlo  proceso_declinacion_lectura
                                 
0007a2 9100 0088                 	lds   r16, OCR1AL								; cuando el valor leido es mayor al techo se decrementa la posicion del servo
0007a4 9110 0089                 	lds   r17, OCR1AH								; decrementando el Output Compare Register y achicando asi el pulso del PWM
                                 	
0007a6 5001                      	subi  r16, 0x01								
0007a7 f418                      	brcc  proceso_declinacion_next2
0007a8 951a                      	dec   r17
                                 
0007a9 3008                      	cpi   r16, 0x08
0007aa f058                      	brlo  proceso_declinacion_fin
                                 
                                 proceso_declinacion_next2:
0007ab 9310 0089                 	sts   OCR1AH, r17								
0007ad 9300 0088                 	sts   OCR1AL, r16
                                 
0007af 0f46                      	add   TECHO_L, PASO								; se define el nuevo techo como TECHO = TECHO + PASO
0007b0 f408                      	brcc  proceso_declinacion_next
0007b1 9553                      	inc   TECHO_H
                                 
                                 proceso_declinacion_next:
0007b2 155c                      	cp    TECHO_H, MEDIDA_H							; se verifica que el valor leido no sea mayor que la medida seleccionada
0007b3 f338                      	brlo  proceso_declinacion_lectura
0007b4 154b                      	cp    TECHO_L, MEDIDA_L
0007b5 f328                      	brlo  proceso_declinacion_lectura
                                 
                                 proceso_declinacion_fin:
0007b6 dddc                      	rcall servo_init								; cuando se llego a la medida seleccionada se lleva el servo a la posicion final
                                 
0007b7 916f                      	pop   PASO
0007b8 915f                      	pop   TECHO_H
0007b9 914f                      	pop   TECHO_L
0007ba 911f                      	pop   r17
0007bb 910f                      	pop   r16
0007bc 9508                      	ret
                                 ;-------------------------------------------------------------------------
                                 ; SEND_COMMAND:
                                 ; Recibe el registro r16 cargado con una configuracion. Luego
                                 ; lo enva a la pantalla LCD.
                                 ;-------------------------------------------------------------------------
                                 send_command:
0007bd 931f                      	push  r17
                                 
0007be 2f10                      	mov   r17, r16				; se mandan los 4 bits mas significativos primero
0007bf 7f10                      	andi  r17, 0xF0
0007c0 b91b                      	out   LCD_DPRT, r17
0007c1 982c                      	cbi   LCD_CPRT, LCD_RS		; RS = 0 es para mandar comandos
0007c2 982b                      	cbi   LCD_CPRT, LCD_RW
0007c3 9a2a                      	sbi   LCD_CPRT, LCD_E
0007c4 d8b0                      	rcall delay_500ns 
0007c5 982a                      	cbi   LCD_CPRT,LCD_E
                                 
0007c6 2f10                      	mov   r17, r16				; ahora los 4 bits restantes
0007c7 9512                      	swap  r17
0007c8 7f10                      	andi  r17, 0xF0
0007c9 b91b                      	out   LCD_DPRT, r17
0007ca 982c                      	cbi   LCD_CPRT, LCD_RS		; RS = 0 es para mandar comandos
0007cb 982b                      	cbi   LCD_CPRT, LCD_RW
0007cc 9a2a                      	sbi   LCD_CPRT, LCD_E
0007cd d8a7                      	rcall delay_500ns 
0007ce 982a                      	cbi   LCD_CPRT,LCD_E
                                 
0007cf d8ae                      	rcall delay_100us
0007d0 911f                      	pop  r17
0007d1 9508                      	ret
                                 ;-------------------------------------------------------------------------
                                 ; SEND_MSG:
                                 ; Recibe el puntero Z cargado con un mensaje guardado en memoria ROM. Luego
                                 ; lo enva por la pantalla LCD.
                                 ;-------------------------------------------------------------------------
                                 send_msg:
0007d2 930f                      	push  r16
0007d3 931f                      	push  r17
                                 
0007d4 e001                      	ldi   r16, CLEAR_DISPLAY
0007d5 dfe7                      	rcall send_command			; Borra la pantalla
0007d6 d8b5                      	rcall delay_3ms
                                 
                                 send_msg_loop:
0007d7 9105                          lpm  r16, Z+
                                 	
0007d8 3f0f                      	cpi  r16, 0xFF				; Me fijo si termino el mensaje
0007d9 f0c9                      	breq send_msg_fin
0007da 300a                      	cpi  r16, '\n'				; Me fijo si el mensaje tiene una segunda parte
0007db f421                      	brne send_msg_next
                                 
0007dc ec00                      	ldi   r16, CURSOR_LINE_2
0007dd dfdf                      	rcall send_command			; Fuerza el cursor a la 2da linea
0007de d8ad                      	rcall delay_3ms	
0007df cff7                      	rjmp  send_msg_loop
                                 
                                 send_msg_next:	
0007e0 2f10                      	mov   r17, r16				; se mandan los 4 bits mas significativos primero
0007e1 7f10                      	andi  r17, 0xF0
0007e2 b91b                      	out   LCD_DPRT, r17
0007e3 9a2c                      	sbi   LCD_CPRT, LCD_RS		; RS = 1 es para mandar datos
0007e4 982b                      	cbi   LCD_CPRT, LCD_RW
0007e5 9a2a                      	sbi   LCD_CPRT, LCD_E
0007e6 d88e                      	rcall delay_500ns 
0007e7 982a                      	cbi   LCD_CPRT,LCD_E
                                 
0007e8 2f10                      	mov   r17, r16				; ahora los 4 bits restantes
0007e9 9512                      	swap  r17
0007ea 7f10                      	andi  r17, 0xF0
0007eb b91b                      	out   LCD_DPRT, r17
0007ec 9a2c                      	sbi   LCD_CPRT, LCD_RS		; RS = 1 es para mandar datos
0007ed 982b                      	cbi   LCD_CPRT, LCD_RW
0007ee 9a2a                      	sbi   LCD_CPRT, LCD_E
0007ef d885                      	rcall delay_500ns 
0007f0 982a                      	cbi   LCD_CPRT,LCD_E
0007f1 d88c                      	rcall delay_100us
0007f2 cfe4                      	rjmp  send_msg_loop
                                 send_msg_fin:		
0007f3 911f                      	pop  r17
0007f4 910f                      	pop  r16
0007f5 9508                      
                                 ;-------------------------------------------------------------------------
                                 ; VARIABLES EN REGISTROS
                                 ;-------------------------------------------------------------------------
                                 .def VASO_H	= r10
                                 .def VASO_M	= r9
                                 .def VASO_L = r8
                                 
                                 ;-------------------------------------------------------------------------
