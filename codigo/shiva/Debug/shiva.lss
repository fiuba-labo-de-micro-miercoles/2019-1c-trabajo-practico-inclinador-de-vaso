                                 ;-------------------------------------------------------------------------
                                 ; DECLINACION_INIT:
                                 ; Funcion que setea el servo en la posicion de mayor inclinacion de la
                                 ; plataforma. Redefine el Output comapre register para lograr un pwm de 2 ms 
                                 ; de tiempo en alto.
                                 ;-------------------------------------------------------------------------
                                 declinacion_init:
000780 930f                      	push r16
                                 	
000781 e001                      	ldi  r16, HIGH(POS_MAYOR_INCLINACION)
000782 9300 0089                 	sts  OCR1AH, r16
                                 
000784 e601                      	ldi  r16, LOW(POS_MAYOR_INCLINACION)
000785 9300 0088                 	sts  OCR1AL, r16
                                 
000787 910f                      	pop  r16
000788 9508                      	ret
                                 
                                 ;-------------------------------------------------------------------------
                                 ; PROCESO_DECLINACION:
                                 ; funcin para el proceso de declinacin del vaso en base de la cantidad
                                 ; de liquido servido. La plataforma (servo) posee 32 posiciones de inclinacin y 
                                 ; el vaso empieza en la posicin 32, y comienza a declinarse cuando
                                 ; la cantidad de lquido servida es igual a la mitad de la medida elegida.
                                 ; Se define un valor PASO = MEDIDA/64 y un TECHO que arranca en PASO,
                                 ; y cada vez que el peso supera el TECHO, se redefine TECHO = TECHO + PASO.
                                 ; Cuando el techo es mas grande que la mitad de la MEDIDA se declina el servo cada
                                 ; vez que se supera TECHO. 
                                 ; Adems, por la pantalla se muestra el avance del proceso a medida que se sirve. 
                                 ; Para ello, cada cuatro incrementos del TECHO, se envia un caracter negro a la pantalla.
                                 ;-------------------------------------------------------------------------
                                 
                                 
                                 proceso_declinacion:
000789 930f                      	push  r16
00078a 936f                      	push  MEDIDA_MITAD_L
00078b 937f                      	push  MEDIDA_MITAD_H
00078c 933f                      	push  TECHO_L
00078d 934f                      	push  TECHO_H
00078e 935f                      	push  PASO
                                 
00078f 2d6b                      	mov   MEDIDA_MITAD_L, MEDIDA_L					; obtenemos la mitad de la medida que es cuando se comienza a declinar el vaso
000790 2d7c                      	mov   MEDIDA_MITAD_H, MEDIDA_H
000791 9576                      	lsr   MEDIDA_MITAD_H							
000792 9567                      	ror   MEDIDA_MITAD_L							
                                 
000793 2f56                      	mov   PASO, MEDIDA_MITAD_L						; se divide el rango de valores que va desde MEDIDA/2 HASTA MEDIDA,
000794 9556                      	lsr   PASO										; por 32, que son las cantidad de posiciones que tiene la paltaforma.
000795 9556                      	lsr   PASO										; que equivale a divir el valor de la MEDIDA por 64.
000796 9556                      	lsr   PASO
000797 9556                      	lsr   PASO
000798 9556                      	lsr   PASO
                                 
000799 2744                      	clr  TECHO_H									
00079a 2733                      	clr  TECHO_L
                                 
                                 proceso_declinacion_loop:
00079b e004                      	ldi   r16, 4									; contador para enviar caracteres que indican el avance del proceso
                                 
                                 proceso_declinacion_techo:
00079c 0f35                      	add   TECHO_L, PASO								; defino el techo como TECHO = TECHO + PASO
00079d f408                      	brcc  proceso_declinacion_techo_next
00079e 9543                      	inc   TECHO_H
                                 
                                 proceso_declinacion_techo_next:
00079f 16c4                      	cp    MEDIDA_H, TECHO_H 						; si MEDIDA_H < TECHO_H ==> MEDIDA < TECHO entonces se termina el proceso
0007a0 f0a0                      	brlo  proceso_declinacion_fin
                                 	
0007a1 154c                      	cp    TECHO_H, MEDIDA_H							; ya se que MEDIDA_H >= TECHO_H, asi que verifico si se cumple la igualdad o el techo es mayor 
0007a2 f010                      	brlo  proceso_declinacion_lectura
                                 	
0007a3 153b                      	cp    TECHO_L, MEDIDA_L							; si esta aca MEDIDA_H = TECHO_H
0007a4 f480                      	brsh  proceso_declinacion_fin					; si TECHO_L > MEDIDA_L => TECHO > MEDIDA y termina el proceso
                                 
                                 proceso_declinacion_lectura:
0007a5 dd5a                      	rcall lectura_peso								; se lee un dato y se escala
0007a6 dda5                      	rcall set_scale
0007a7 de16                      	rcall detectar_cancelacion
                                 
0007a8 1634                      	cp    DATO_M, TECHO_H							; se compara el valor leido con el techo
0007a9 f3d8                      	brlo  proceso_declinacion_lectura				; mientras el dato sea menor que el techo se sigue leyendo 
0007aa 1623                      	cp    DATO_L, TECHO_L
0007ab f3c8                      	brlo  proceso_declinacion_lectura
                                 	
0007ac 1774                      	cp    MEDIDA_MITAD_H, TECHO_H					; se compara el dato leido con la mitad de la medida para 
