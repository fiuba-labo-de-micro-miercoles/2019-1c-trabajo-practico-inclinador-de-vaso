                                 ; LECTURA_PESO: 
                                 ; funcion para la lectura de datos de la celda de carga. Carga
                                 ; los bits enviados por el amplificador HX711 a traves del pin DOUT y los
                                 ; guarda en los registros r4:r2 y les resta el tara. Usa los registros r16 y r17
                                 ;-------------------------------------------------------------------------
                                 
                                 lectura_peso:
000500 930f                      	push  r16
000501 931f                      	push  r17
                                 
                                 lectura_peso_loop:
000502 9931                      	sbic  PINC, DOUT					
000503 cffe                      	rjmp  lectura_peso_loop			; chequeo si DOUT est en alto
000504 0000                      	nop							
000505 0000                      	nop 							; Espero dos ciclos y vuelvo a preguntar si sigue en alto ya que
000506 9931                      	sbic  PINC, DOUT				; por la hoja de datos, DOUT debe estar como minimo 0.1 useg en 0
000507 cffa                      	rjmp  lectura_peso_loop			; para indicar que tiene un dato disponible para mandar.
                                 
000508 d012                      	rcall cargar_byte				; lee 8 bits y los guarda en r16
000509 2e40                      	mov   DATO_H, r16				; primer byte es el mas significativo
00050a d010                      	rcall cargar_byte
00050b 2e30                      	mov   DATO_M, r16
00050c d00e                      	rcall cargar_byte
00050d 2e20                      	mov   DATO_L, r16
                                 
00050e 9a40                      	sbi   PORTC, SCK				; se genera el pulso numero 25 requerido para setear el DOUT
00050f 0000                      	nop								; debe estar un tiempo en alto mayor a 0.2 useg
000510 0000                      	nop
000511 0000                      	nop
000512 0000                      	nop
000513 9840                      	cbi   PORTC, SCK
                                 	
000514 d05e                      	rcall com_2						; Hace el complemento a 2 de los datos leidos
                                    	
000515 1825                      	sub   DATO_L, TARA_L			; Le saco el offset a los valores
000516 0836                      	sbc   DATO_M, TARA_M
000517 0847                      	sbc   DATO_H, TARA_H
                                 	
000518 911f                      	pop   r17
000519 910f                      	pop   r16
00051a 9508                      	ret
                                 
                                 ;-------------------------------------------------------------------------
                                 ; CARGAR_BYTE: 
                                 ; Carga 8 bits del amplificador uno por uno y los guarda en r16.
                                 ;-------------------------------------------------------------------------
                                 
                                 cargar_byte:
00051b 2700                      	clr   r16
00051c e018                      	ldi   CONT_8, 8
                                 cargar_bit:
00051d 9a40                      	sbi   PORTC, SCK				; se genera un flanco ascendente en la seal SCK para cargar un bit
00051e 0000                      	nop
00051f 0000                      	nop
000520 0f00                      	lsl   r16
000521 9931                      	sbic  PINC, DOUT				; si en DOUT hay un 1, se incrementa r16 y pone un 1 en el LSB	
000522 9503                      	inc   r16						; si en DOUT hay un 0, salte esta instruccion y deja un 0 en LSB	
000523 9840                      	cbi   PORTC, SCK				; se genera un flanco descendente en la seal SCK para cargar un bit
000524 951a                      	dec   CONT_8
000525 f7b9                      	brne  cargar_bit
000526 9508                      	ret
                                 
                                 ; ----------------------------------------------------------------------
                                 ; SET_TARA:
                                 ; Esta funcion lee 16 valores, calcula el promedio y es el offset que hay que restarle 
                                 ; luego a cada dato leido.
                                 ; me deja en TARA_H, TARA_M, TARA_L el peso leido inicialmente.
                                 ; ----------------------------------------------------------------------
                                 set_tara:
000527 930f                      	push r16
000528 931f                      	push r17
000529 932f                      	push r18
00052a 933f                      	push r19
00052b 934f                      	push r20
                                 
00052c e100                      	ldi  r16, 16					; contador, se va a hacer un promedio de 16 muestras para el offset
00052d 2455                      	clr  TARA_L					
00052e 2466                      	clr  TARA_M
00052f 2477                      	clr  TARA_H					
000530 2711                      	clr  r17						; 4 bytes para ir guardando la suma parcial del promedio
000531 2722                      	clr  r18
000532 2733                      	clr  r19		
000533 2744                      	clr  r20
                                 
                                 set_tara_loop:					
000534 dfcb                      	rcall lectura_peso			
000535 0d12                      	add  r17, DATO_L			; se usan registros auxiliares para no modificar las lecturas dentro de
000536 1d23                      	adc  r18, DATO_M			; lectura peso, ya que se resta en esa funcion la tara
000537 1d34                      	adc  r19, DATO_H
000538 f408                      	brcc set_tara_next
000539 9543                      	inc  r20 					; si hubo carry incremento el HIGH_BYTE
                                 set_tara_next:
00053a 950a                      	dec  r16
00053b f7c1                      	brne set_tara_loop
                                 
                                 set_tara_division_por_4:	
00053c e004                      	ldi  r16, 4					; Para dividir por 16 se crea un contador con 4 porque se va a shiftear 4 veces
                                 set_tara_division:
00053d 9546                      	lsr  r20
00053e 9537                      	ror  r19
00053f 9527                      	ror  r18
000540 9517                      	ror  r17
000541 950a                      	dec  r16
000542 f7d1                      	brne  set_tara_division
                                 	
000543 2e73                      	mov TARA_H, r19				; guardo en tara el promedio de los primeros 16 valores
000544 2e62                      	mov TARA_M, r18
000545 2e51                      	mov TARA_L, r17
                                 	
000546 914f                      	pop r20
000547 913f                      	pop r19
000548 912f                      	pop r18
000549 911f                      	pop r17
00054a 910f                      	pop r16	
00054b 9508                      	ret
                                 
                                 ; ----------------------------------------------------------------------
                                 ; SET_SCALE:
                                 ; Con los datos de la balanza obtuvimos el factor de escala para pasar
                                 ; los datos a gramos. Como el factor es 1/211.9170459 que se aproxim a 77/2^14
                                 ; ----------------------------------------------------------------------
                                 
                                 set_scale:
00054c 930f                      	push r16
00054d 931f                      	push r17
00054e 932f                      	push r18
00054f 933f                      	push r19
000550 934f                      	push r20
                                 
000551 2711                      	clr r17
000552 2722                      	clr r18
000553 2733                      	clr r19
000554 2744                      	clr r20
                                 	
000555 e40d                      	ldi  r16, MULTIPLICADOR
000556 2044                      	tst  DATO_H								; si el valor leido es negativo (tara>valor_leido) se le hace el complemento a 2
000557 f40a                      	brpl multiplicacion_low
000558 d01a                      	rcall com_2
                                 											; primero multiplicamos por 77 el dato y guardamos el resultado en r20:r17
                                 multiplicacion_low:
000559 9e20                      	mul  DATO_L, r16						; mutiplico el byte bajo del numero con 3 bytes
00055a 2d21                      	mov  r18, r1
00055b 2d10                      	mov  r17, r0
                                 	
                                 multiplicacion_middle:
00055c 9e30                      	mul  DATO_M, r16						; mutiplico el byte medio del numero con 3 bytes
00055d 0d20                      	add  r18, r0
00055e 1d31                      	adc  r19, r1
00055f f408                      	brcc multiplicacion_high
000560 9543                      	inc  r20
                                 
                                 multiplicacion_high:						; mutiplico el byte alto del numero con 3 bytes
000561 9e40                      	mul  DATO_H, r16
000562 0d30                      	add  r19, r0
000563 1d41                      	adc  r20, r1
                                 											; vamos a dividir por 2^14, que es tomar los primero 3 bytes y shiftear 6 veces
000564 2e22                      	mov  DATO_L, r18
000565 2e33                      	mov  DATO_M, r19
000566 2e44                      	mov  DATO_H, r20
                                 
                                 division_por_6:
000567 e006                      	ldi  r16, 6
                                 
                                 shifteo_loop:
000568 9446                      	lsr  DATO_H
000569 9437                      	ror  DATO_M
00056a 9427                      	ror  DATO_L
00056b 950a                      	dec  r16
00056c f7d9                      	brne shifteo_loop
                                 	
00056d 914f                      	pop r20
00056e 913f                      	pop r19
00056f 912f                      	pop r18
000570 911f                      	pop r17
000571 910f                      	pop r16
000572 9508                      	ret
                                 
                                 ; ----------------------------------------------------------------------
                                 ; COM_2:
                                 ; Calcula el complemento a dos del dato guardado en  r4:r2 
                                 ; ----------------------------------------------------------------------
                                 com_2:
000573 930f                      	push  r16
                                 	
000574 2700                      	clr r16	
000575 9440                      	com DATO_H
000576 9430                      	com DATO_M
000577 9421                      	neg DATO_L
000578 f010                      	brcs com_2_retornar
000579 1e30                      	adc  DATO_M, r16
00057a 1e40                      	adc  DATO_H, r16
                                 
                                 com_2_retornar:
00057b 910f                      	pop r16
00057c 9508                      	ret
                                 
                                 .include "configuracion.asm"
                                 
                                 ; CONFIGURACION_PUERTOS:
                                 ; se definen los puertos de entrada o de salda para todo el programa
                                 ;-------------------------------------------------------------------------
                                 configuracion_puertos:
00057d 930f                      	push r16
                                 									; CELDA DE CARGA	
00057e 9a38                      	sbi  DDRC, SCK					; puerto PC0 = A0 como salida (SCK)
00057f 9839                      	cbi  DDRC, DOUT
000580 9a41                      	sbi  PORTC, DOUT				; puerto PC = A1 como entrada (DOUT) 
                                 
                                 									; SERVO
000581 9a21                      	sbi  DDRB, PB1					; PB1 como output pin						
                                 	
000582 ef00                      	ldi  r16, 0xF0					; PANTALLA LCD		
000583 b90a                      	out  LCD_DDDR, r16				; Configuro los bits D7:D4 del puerto D como de salida
000584 9a24                      	sbi  LCD_CDDR, LCD_RS		
000585 9a23                      	sbi  LCD_CDDR, LCD_RW
000586 9a22                      	sbi  LCD_CDDR, LCD_E			; Se configura los puertos de dato (PORTD) y los comandos usados (del PORTB) como de salida 
000587 db0b                      	rcall delay_3ms
                                 
000588 910f                      	pop  r16
000589 9508                      	ret											
                                 
                                 ;-------------------------------------------------------------------------
                                 ; LCD_INIT:
                                 ; se define el formato en que se le envian los datos a la pantalla, y el 
                                 ; formato del cursor.
                                 ;-------------------------------------------------------------------------
                                 LCD_init:
00058a 930f                      	push  r16
                                 
00058b e208                      	ldi   r16, DATA_4BIT			; Trabajamos enviando los caracteres de a 4 bits
00058c d244                      	rcall send_command
00058d db05                      	rcall delay_3ms
                                 	
00058e e00c                      	ldi   r16, DISP_ON_CURS_OFF		; Sin cursor
00058f d241                      	rcall send_command				
000590 db02                      	rcall delay_3ms
                                 
000591 910f                      	pop   r16
000592 9508                      	ret
                                 
                                 ;-------------------------------------------------------------------------
                                 ; SERVO_INIT:
                                 ; Inicializa la posicin del servo tal que la plataforma se encuentre en
                                 ; posicin vertical. Envia un PWM de periodo 20 ms y tiempo en alto 0,5 ms
                                 ; por el pin correspondiente al TIMER1A
                                 ;-------------------------------------------------------------------------
                                 servo_init:
000593 930f                      	push r16
                                 
000594 e001                      	ldi  r16, HIGH(313 + 7)
000595 9300 0089                 	sts  OCR1AH, r16
                                 
000597 e400                      	ldi  r16, LOW(313 + 7)
000598 9300 0088                 	sts  OCR1AL, r16
                                 
00059a e803                      	ldi  r16, (1<<WGM11)|(1<<WGM10)|(1<<COM1A1)|(0<<COM1A0)			; fast-PWM mode, non-inverting mode
00059b 9300 0080                 	sts  TCCR1A, r16					
                                 
00059d e00d                      	ldi  r16, (0<<WGM13)|(1<<WGM12)|(1<<CS02)|(0<<CS01)|(1<<CS00)
00059e 9300 0081                 	sts  TCCR1B, r16												; con prescaler = 1024
                                 
0005a0 e001                      	ldi  r16, (1<<TOIE1)
0005a1 9300 006f                 	sts  TIMSK1, r16												; se activa interrupciones
                                 
0005a3 910f                      	pop  r16
0005a4 9508                      	ret
                                 .include "input.asm"
                                 
                                 ; DETECTAR_PERTURBACION:
                                 ; Para la configuracion del dispositivo se detectaran golpes dados por
                                 ; el usuario. Entonces, se determin, a partir de mediciones, que un golpe equivale a aproximadamente
                                 ; 300 gr. Por lo que se toma el byte del medio del dato y se verifica si es
                                 ; mayor a 255. Si lo es se verifica que haya un segundo golpe. Si esto
                                 ; ocurre significa que el dispositivo esta siendo configurado.
                                 ; ----------------------------------------------------------------------
                                 detectar_perturbacion:
0005a5 930f                      	push  r16
0005a6 931f                      	push  r17
                                 
0005a7 e001                      	ldi	  r16, HIGH(MIN_PERTURBACION)					; se utiliza para comparar el valor de lectura y saber si hubo alguna perturbacin
                                 	
                                 detectar_perturbacion_lectura:
0005a8 f096                      	brts  detectar_perturbacion_fin						; se utiliza para la funcin CONFIGURAR_MEDIDA
                                 	
0005a9 df56                      	rcall lectura_peso
0005aa dfa1                      	rcall set_scale	
0005ab 1630                      	cp    DATO_M, r16
0005ac f3d8                      	brlo  detectar_perturbacion_lectura
0005ad e014                      	ldi   r17, 4
                                 
                                 detectar_perturbacion_verificacion:
0005ae 951a                      	dec   r17
0005af f3c1                      	breq  detectar_perturbacion_lectura
0005b0 df4f                      	rcall lectura_peso
0005b1 df9a                      	rcall set_scale
0005b2 1630                      	cp    DATO_M, r16
0005b3 f7d0                      	brsh  detectar_perturbacion_verificacion
0005b4 e016                      	ldi   r17, NRO_MUESTRAS
                                 
                                 detectar_perturbacion_verificacion2:
0005b5 951a                      	dec   r17
0005b6 f389                      	breq  detectar_perturbacion_lectura
0005b7 df48                      	rcall lectura_peso
0005b8 df93                      	rcall set_scale
0005b9 1630                      	cp    DATO_M, r16
0005ba f3d0                      	brlo  detectar_perturbacion_verificacion2
                                 
                                 detectar_perturbacion_fin:
0005bb 911f                      	pop   r17
0005bc 910f                      	pop   r16
0005bd 9508                      	ret
                                 
                                 ; ----------------------------------------------------------------------
                                 ; DETECTAR_CANCELACION:
                                 ; La cancelacin se da en cualquier momento del proceso cuando el usuario
                                 ; hace una presin de ms de 4096 g. Esta funcin chequea el dato que recibe en DATO, 
                                 ; lo compara con 4096 g (0x1000) y si es es mayor, espera a que no haya nada  
                                 ; sobre la balanza y vuelve a setup. Se eligi 4096 g porque es solo comparar el segundo
                                 ; byte del dato con 0x10.
                                 ; ----------------------------------------------------------------------	
                                 detectar_cancelacion:	
0005be 930f                      	push r16
                                 
0005bf 2d03                      	mov  r16, DATO_M
0005c0 3100                      	cpi  r16, HIGH (VALOR_CANCELACION)
0005c1 f410                      	brsh proceso_cancelado
                                 	
0005c2 910f                      	pop  r16
0005c3 9508                      	ret
                                 
                                 proceso_cancelado:						
0005c4 931f                      	push  r17
                                 	
0005c5 e0fe                      	ldi   zh, HIGH(DIR_MSG_CANCELACION<<1)
0005c6 eee6                      	ldi   zl, LOW(DIR_MSG_CANCELACION<<1)
0005c7 d21e                      	rcall send_msg
                                 										; se debe volver al SETUP cuando no haya peso sobre la balanza
                                 										; y eso se dara cuando el dato sea igual al peso del vaso 
                                 										; que quedo guardado en VASO
                                 proceso_cancelado_loop:	
0005c8 e016                      	ldi    r17, NRO_MUESTRAS
0005c9 e004                      	ldi    r16, MARGEN_ERROR
                                 proceso_cancelado_lectura:
0005ca df35                      	rcall  lectura_peso	
0005cb df80                      	rcall  set_scale
                                 
0005cc 1828                      	sub    DATO_L, VASO_L				; se calcula el mdulo de la resta del DATO y el peso del VASO
0005cd 0839                      	sbc    DATO_M, VASO_M
0005ce 084a                      	sbc	   DATO_H, VASO_H
0005cf f408                      	brsh   proceso_cancelado_next
0005d0 dfa2                      	rcall  com_2
                                 
                                 proceso_cancelado_next:	
0005d1 2044                      	tst    DATO_H
0005d2 f7a9                      	brne   proceso_cancelado_loop
0005d3 2033                      	tst    DATO_M
0005d4 f799                      	brne   proceso_cancelado_loop
0005d5 1620                      	cp     DATO_L, r16
0005d6 f788                      	brsh   proceso_cancelado_loop
0005d7 951a                      	dec    r17
0005d8 f789                      	brne   proceso_cancelado_lectura
                                 	
0005d9 911f                      	pop    r17
0005da 910f                      	pop    r16
0005db 940c 0034                 	jmp    main
                                 
                                 ;-------------------------------------------------------------------------
                                 ; DETECTAR_VASO:
                                 ; Se tiene en cuenta que un vaso de virdio pesa aproximadamente 200gr. 
                                 ; La funcin lee un dato y compara con el valor mnimo de un vaso, 
                                 ; cuando detecta este valor se fija que en la siguientes lecturas que
                                 ; el valor sea aproximadamente el mismo. Guarda ese valor en VASO_H:VASO_L
                                 ; y setea la tara. 
                                 ;-------------------------------------------------------------------------
                                 
                                 detectar_vaso: 
0005dd 930f                      	push  r16
0005de 931f                      	push  r17
0005df 932f                      	push  r18
0005e0 933f                      	push  r19
0005e1 934f                      	push  r20
0005e2 935f                      	push  r21
                                 
0005e3 ec08                      	ldi   r16, VASO_MINIMO				; peso aproximado de un vaso de vidrio
0005e4 e014                      	ldi   r17, MARGEN_ERROR
                                 
                                 detectar_vaso_loop:
0005e5 e026                      	ldi   r18, NRO_MUESTRAS				; contador
                                 detectar_vaso_lectura:
0005e6 df19                      	rcall lectura_peso					; lee un dato y lo escala
0005e7 df64                      	rcall set_scale						
0005e8 dfd5                      	rcall detectar_cancelacion
                                 	
0005e9 2033                      	tst   DATO_M						
0005ea f411                      	brne  detectar_vaso_verificacion					
0005eb 1620                      	cp    DATO_L, r16					; compara el peso leido con el peso estandar de un vaso			
0005ec f3c8                      	brlo  detectar_vaso_lectura			; si no detecta un cambio vuelve a leer un peso
                                 	
                                 detectar_vaso_verificacion:				; la balanza envia 10 muestras por segundo por lo que la siguiente muestra sera a los 10 ms de la anterior
0005ed 2d32                      	mov   r19, DATO_L
0005ee 2d43                      	mov   r20, DATO_M
0005ef 2d54                      	mov   r21, DATO_H
                                 
0005f0 df0f                      	rcall lectura_peso					; lee un dato y lo escala
0005f1 df5a                      	rcall set_scale						
0005f2 dfcb                      	rcall detectar_cancelacion
                                 	
0005f3 1a23                      	sub   DATO_L, r19					; compara el siguiente valor leido con el anterior			
0005f4 0a34                      	sbc   DATO_M, r20
0005f5 0a45                      	sbc   DATO_H, r21
                                 	
0005f6 f408                      	brsh  detectar_vaso_next
0005f7 df7b                      	rcall com_2							; se toma el modulo de la diferencia de dos valores consecutivos
                                 
                                 detectar_vaso_next:
0005f8 2044                      	tst   DATO_H
0005f9 f759                      	brne  detectar_vaso_loop
0005fa 2033                      	tst   DATO_M
0005fb f749                      	brne  detectar_vaso_loop
0005fc 1621                      	cp    DATO_L, r17
0005fd f738                      	brsh  detectar_vaso_loop
0005fe 952a                      	dec   r18
0005ff f731                      	brne  detectar_vaso_lectura
                                 
000600 2e83                      	mov   VASO_L, r19
000601 2e94                      	mov   VASO_M, r20
000602 2ea5                      	mov   VASO_H, r21
                                 
000603 e0fe                      	ldi   zh, HIGH(DIR_MSG_AGUARDE<<1)	; Mensaje hasta que se setea la nueva tara
000604 e2ee                      	ldi   zl, LOW(DIR_MSG_AGUARDE<<1)
000605 d1e0                      	rcall send_msg
                                 
000606 df20                      	rcall set_tara						; setea la tara ahora con el vaso puesto
                                 	
000607 915f                      	pop   r21
000608 914f                      	pop   r20
000609 913f                      	pop   r19
00060a 912f                      	pop   r18
00060b 911f                      	pop   r17
00060c 910f                      	pop   r16
00060d 9508                      	ret
                                 	
                                 ;-------------------------------------------------------------------------
                                 ; CONFIGURAR_MEDIDA:
                                 ; Se espera 4 segundos a que el usuario golpee 2 veces para cambiar la medida.
                                 ; De otra forma, se selecciona la medida elegida y se guarda el valor en MEDIDA.
                                 ;-------------------------------------------------------------------------
                                 configurar_medida:
00060e 930f                      	push r16
00060f 931f                      	push r17
000610 932f                      	push r18
000611 93ef                      	push zl
000612 93ff                      	push zh
                                 
000613 ef10                      	ldi  r17, END_OF_MESSAGE
                                 
                                 configurar_medida_init:
000614 e0fe                      	ldi  zh, HIGH(DIR_MSG_PINTA<<1)
000615 e5e8                      	ldi  zl, LOW(DIR_MSG_PINTA<<1)		; Se inicializa en la medida pinta
                                 	
                                 configurar_medida_loop:
000616 d1cf                      	rcall send_msg						; envia el mensaje correspondiente a la medida cargada
                                 
000617 90b5                      	lpm  MEDIDA_L, Z+					; se guarda la medida seleccionada, cuyo valor esta a continuacion de su mensaje
000618 90c5                      	lpm  MEDIDA_H, Z+ 
                                 	
000619 16b1                      	cp   MEDIDA_L, r17					; si el siguiente valor es 0xF0, ya se recorrieron las 3 medidas
00061a f3c9                      	breq configurar_medida_init			; vuelve a comenzar por PINTA
                                 		
00061b da85                      	rcall delay_4s						; inicializa el delay de 4 seg
                                 	
00061c df88                      	rcall detectar_perturbacion			; se queda esperando una perturbacin, si en 4 segundos no detecto nada ya se seleccion una medida.
00061d f026                      	brts  configurar_medida_fin			; si esta el bit T=1 significa que pasaron 4 segundos dentro de la funcin detectar_perturbacion 
                                 	                                    ; y sale de la funcin con la medida seleccionada en MEDIDA
                                 
00061e e000                      	ldi   r16, (0<<TOIE0)				; se desactiva la interrupcin por overflow
00061f 9300 006e                 	sts   TIMSK0, r16
                                 	
000621 cff4                      	rjmp  configurar_medida_loop
                                  
                                 configurar_medida_fin:					; si pasaron 4 seg, de la subrutina de interrupcin se vuelve aqu	
000622 e000                      	ldi   r16, (0<<CS02)|(0<<CS01)|(0<<CS00)
000623 bd05                      	out   TCCR0B, r16					; config: se apaga el timer
                                 
000624 e000                      	ldi  r16, (0<<TOIE0)				; se desactiva la interrupcin por overflow
000625 9300 006e                 	sts  TIMSK0, r16
                                 	
000627 94e8                      	clt									; se pone en cero el flag T						
                                 
000628 91ff                      	pop   zh
000629 91ef                      	pop   zl
00062a 912f                      	pop   r18
00062b 911f                      	pop   r17
00062c 910f                      	pop   r16
00062d 9508                      	ret	
                                 
                                 ;-------------------------------------------------------------------------
                                 ; FIN_PROGRAMA:
                                 ; funcin que aguarda a que retiren el vaso de la plataforma. Considerando
                                 ; que el peso del vaso quedo guardado en VASO_H:VASO_L, la funcin lee datos
                                 ; de la celda hasta encontrar NRO_MUESTRAS muestras consecutivas cuyo valor
                                 ; sean iguales a VASO con un margen de error de MARGEN_ERROR
                                 ;-------------------------------------------------------------------------
                                 
                                 fin_programa:
00062e 930f                      	push  r16
00062f 931f                      	push  r17
                                 
000630 e014                      	ldi   r17, MARGEN_ERROR
                                 				
                                 fin_programa_loop:
000631 e006                      	ldi   r16, NRO_MUESTRAS
                                 fin_programa_lectura:
000632 decd                      	rcall lectura_peso
000633 df18                      	rcall set_scale
                                 
000634 1828                      	sub   DATO_L, VASO_L							
000635 0839                      	sbc   DATO_M, VASO_M
000636 084a                      	sbc   DATO_H, VASO_H
                                 
000637 f408                      	brsh  fin_programa_next
000638 df3a                      	rcall com_2							; se obtiene el modulo de la resta
                                 
                                 fin_programa_next:
000639 2044                      	tst   DATO_H
00063a f7b1                      	brne  fin_programa_loop
00063b 2033                      	tst   DATO_M
00063c f7a1                      	brne  fin_programa_loop
00063d 1621                      	cp    DATO_L, r17
00063e f790                      	brsh  fin_programa_loop				; se compara el modulo de la resta con el marge de error
00063f 950a                      	dec   r16
000640 f789                      	brne  fin_programa_lectura			; cuando se leen NRO_MUESTRAS datos, cuya resta con el peso del vaso es menor al margen 
                                 										; de error, es porque retiraron el vaso
000641 911f                      	pop  r17
000642 910f                      	pop  r16
